msgstr "Zwingt einen User einen oder mehrere Channel zu betreten"

msgid "<nick> <channel>[,<channel>...]"
msgstr "<nick> <channel>[,<channel>...]"

msgid ""
"   nick: nick\n"
"channel: channel name"
msgstr ""
"   nick: Nickname\n"
"channel: Name des Channels"

